<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="cn.mldn.dibmp.dao.IStorageApplyDetailsDAO">
  <insert id="doCreate" parameterType="StorageApplyDetails">
  	INSERT INOT storage_apply_details(gid,name,num,price,weight)
  	VALUES (#{gid},#{name},#{num},#{price},#{weight})
  </insert>
  
  <select id="findBySadid" parameterType="StorageApplyDetails" resultType="Long">
  	SELECT sadid,gid,name,num,price,weight
  	FROM storage_apply_details WHERE said=${sadid}
  </select>
</mapper>
